#!/usr/bin/env bash
# Stops the 4-to_infinity_and_beyond script using the kill command

# Find the PID of the script
PID=$(pgrep -f 4-to_infinity_and_beyond)

# Kill the process if found
if [ -n "$PID" ]; then
  kill "$PID"
  echo "Stopped 4-to_infinity_and_beyond (PID: $PID)"
else
  echo "No such process running."
fi
